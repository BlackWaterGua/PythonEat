<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane onKeyPressed="#handle" prefHeight="500.0" prefWidth="400.0" style="-fx-background-color: white;" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="snake.PauseMenuController">
   <children>
      <Label layoutX="123.0" layoutY="50.0" text="PAUSE">
         <font>
            <Font size="49.0" />
         </font>
      </Label>
      <Button fx:id="Exit" layoutX="149.0" layoutY="399.0" mnemonicParsing="false" onAction="#Exit" prefHeight="63.0" prefWidth="102.0" text="EXIT">
         <font>
            <Font size="22.0" />
         </font>
      </Button>
      <Button fx:id="Menu" layoutX="149.0" layoutY="311.0" mnemonicParsing="false" onAction="#Menu" prefHeight="63.0" prefWidth="102.0" text="MENU">
         <font>
            <Font size="22.0" />
         </font>
      </Button>
      <Button fx:id="Continue" layoutX="123.0" layoutY="137.0" mnemonicParsing="false" onAction="#Continue" prefHeight="63.0" prefWidth="154.0" text="CONTINUE">
         <font>
            <Font size="22.0" />
         </font>
      </Button>
      <Button fx:id="Restart" layoutX="119.0" layoutY="225.0" mnemonicParsing="false" onAction="#Restart" prefHeight="63.0" prefWidth="162.0" text="RESTART">
         <font>
            <Font size="22.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
